{
  "name": "Agent",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "firebaseUid": {
      "type": "string",
      "required": true
    },
    "profilePicture": {
      "type": "string"
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "averageRating": {
      "type": "number",
      "required": true,
      "default": 4
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    },
    "clients": {
      "type": "hasMany",
      "model": "Client",
      "foreignKey": ""
    },
    "reviews": {
      "type": "hasMany",
      "model": "Review",
      "foreignKey": ""
    },
    "insuranceCompany": {
      "type": "belongsTo",
      "model": "InsuranceCompany",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
